    {
        "id": "38503a7f.593d06",
        "type": "function",
        "z": "5f562f66.a301e",
        "name": "parse initSettings",
        "func": "function getAttrObject(device, propName, attrName)\n{\n    var props = device[propName];\n    var attr = props.find(attr => attr.attr === attrName);\n    return attr;\n}\n\nvar device = msg.payload;\nfor (var ii = 0; ii < msg.payload.settings.length; ++ii)\n{\n    if (msg.payload.settings[ii].type == 'scalar')\n    {\n        msg.payload.settings[ii].alarm.value = 0;\n        if (!isNaN(msg.payload.settings[ii].value))\n        {\n            if (msg.payload.settings[ii].value < msg.payload.settings[ii].alarm.limits.low)  msg.payload.settings[ii].alarm.value = 1;\n            if (msg.payload.settings[ii].value < msg.payload.settings[ii].alarm.limits.lolo) msg.payload.settings[ii].alarm.value = 3;\n            if (msg.payload.settings[ii].value > msg.payload.settings[ii].alarm.limits.high) msg.payload.settings[ii].alarm.value = 2;\n            if (msg.payload.settings[ii].value > msg.payload.settings[ii].alarm.limits.hihi) msg.payload.settings[ii].alarm.value = 4;\n        }\n    }\n}\n\nflow.set('device', msg.payload);    \n\nreturn [\n    {topic:'chanOn0',       payload:getAttrObject(device, 'settings', 'on-dipole').value},\n    {topic:'timeOffset0',   payload:getAttrObject(device, 'settings', 'filter-dipole').value},\n    {topic:'sign0',         payload:getAttrObject(device, 'settings', 'sign-dipole').value},\n    {topic:'maxAcValue0',   payload:getAttrObject(device, 'settings', 'max-ac-dipole').value},\n    {topic:'gain0',         payload:getAttrObject(device, 'settings', 'gain-dipole').value},\n    {topic:'dacOffset0',    payload:getAttrObject(device, 'settings', 'offset-dipole').value},\n    {topic:'chanOn1',       payload:getAttrObject(device, 'settings', 'on-quad').value},\n    {topic:'timeOffset1',   payload:getAttrObject(device, 'settings', 'filter-quad').value},\n    {topic:'sign1',         payload:getAttrObject(device, 'settings', 'sign-quad').value},\n    {topic:'maxAcValue1',   payload:getAttrObject(device, 'settings', 'max-ac-quad').value},\n    {topic:'gain1',         payload:getAttrObject(device, 'settings', 'gain-quad').value},\n    {topic:'dacOffset1',    payload:getAttrObject(device, 'settings', 'offset-quad').value},\n    ];\n",
